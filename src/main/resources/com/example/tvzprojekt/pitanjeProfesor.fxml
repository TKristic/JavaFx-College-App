<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-border-color: transparent;" styleClass="dashboard" stylesheets="@../../../stylesheet.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tvzprojekt.Controllers.PitanjeProfesor">
   <children>
      <VBox prefHeight="150.0" prefWidth="300.0">
         <children>
            <Label prefHeight="90.0" prefWidth="300.0" text="Unosite li trenutnu listu ili unosite vanjsku datoteku" textAlignment="CENTER" textFill="WHITE" wrapText="true">
               <font>
                  <Font name="Ebrima Bold" size="18.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="12.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#input" text="Input" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="61.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#output" text="Output" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="cancel" onAction="#cancelDialog" mnemonicParsing="false" text="Odustani" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="9.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
